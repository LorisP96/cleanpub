<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
  <type-extension type-id="Product">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="sku">
                <display-name xml:lang="x-default">sku</display-name>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
                <attribute-definition attribute-id="minimumPrice">
                <display-name xml:lang="x-default">minimumPrice</display-name>
                <type>decimal</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>0</default-value>
            </attribute-definition>
                <attribute-definition attribute-id="suggestedPrice">
                <display-name xml:lang="x-default">suggestedPrice</display-name>
                <type>decimal</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>1</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="fullfilment">
                <display-name xml:lang="x-default">fullfilment</display-name>
                <type>decimal</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>0</default-value>
            </attribute-definition>
            <group-definitions>
                <attribute-group group-id="bookProduct">
                    <display-name xml:lang="x-default">bookProduct</display-name>
                        <attribute attribute-id="sku"/>
                        <attribute attribute-id="minimumPrice"/>
                        <attribute attribute-id="suggestedPrice"/>
                        <attribute attribute-id="fullfilment"/>
                </attribute-group>
        </group-definitions>
        </custom-attribute-definitions>
    </type-extension>
</metadata>