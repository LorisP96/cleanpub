<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
<custom-type type-id="book">
    <display-name xml:lang="x-default">bookProduct</display-name>
    <description xml:lang="x-default">book of the author</description>
    <staging-mode>no-staging</staging-mode>
    <storage-scope>site</storage-scope>
    <retention-days>365</retention-days>
    <key-definition attribute-id="sku">
        <type>string</type>
        <mandatory-flag>true</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="idAutore">
        <type>string</type>
        <mandatory-flag>true</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="name">
        <type>string</type>
        <mandatory-flag>false</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="description">
        <type>string</type>
        <mandatory-flag>false</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="minimumPrice">
        <type>decimal</type>
        <mandatory-flag>true</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="suggestedPrice">
        <type>decimal</type>
        <mandatory-flag>true</mandatory-flag>
        <min-greater-than>minimumPrice</min-greater-than>
        <min-length>0</min-length>
    </key-definition>
    <key-definition attribute-id="fullfilment">
        <type>decimal</type>
        <mandatory-flag>true</mandatory-flag>
        <min-length>0</min-length>
    </key-definition>
    <group-definitions>
        <attribute-group group-id="general">
            <display-name xml:lang="x-default">General</display-name>
            <attribute attribute-id="email"/>
        </attribute-group>
    </group-definitions>
</custom-type>
</metadata>