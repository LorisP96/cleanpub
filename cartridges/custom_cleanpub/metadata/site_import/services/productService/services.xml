<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="productService_Credentials">
        <url></url>
        <user-id></user-id>
        <password></password>
    </service-credential>

    <service-profile service-profile-id="productService_Profile">
        <timeout-millis>30000</timeout-millis>
        <cb-enabled>false</cb-enabled>
    </service-profile>

    <service service-id="data.api.create.product">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>productservice_</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>productService_Profile</profile-id>
        <credential-id>productService_Credentials</credential-id>
    </service>
</services>